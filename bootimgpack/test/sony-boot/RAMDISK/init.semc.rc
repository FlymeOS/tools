on property:vold.decrypt=trigger_post_fs_data
    mkdir /data/idd 0755 idd idd
    mount ext4 /dev/block/mmcblk0p10 /data/idd nosuid nodev noatime barrier=0
    chown idd idd /data/idd
service hdmid /system/bin/hdmid
    class main
    user system
