service ccci_fsd /system/bin/ccci_fsd
    user root
    oneshot

service ccci_mdinit /system/bin/ccci_mdinit
    user root
    oneshot

service nvram_daemon /system/bin/nvram_daemon
    user root
    group nvram
    oneshot

#
# MT6620 related services (Begin)
